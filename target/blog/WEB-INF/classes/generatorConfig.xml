<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>

	<!--指定特定数据库的jdbc驱动jar包的位置 -->
	<classPathEntry location="C:\Users\cjdai\Desktop\generator\Oracle11g.jar" />
	<context id="DB2Tables" targetRuntime="MyBatis3">
		<commentGenerator>
			<!-- 是否去除自动生成的注释 true：是 ： false:否 -->
			<property name="suppressAllComments" value="true" />
			<!--数据库连接的信息：驱动类、连接地址、用户名、密码 -->
		</commentGenerator>
		<jdbcConnection driverClass="oracle.jdbc.driver.OracleDriver"
			connectionURL="jdbc:oracle:thin:@192.168.0.241:1521:ora11g" 
			userId="bjdata"
			password="bjdata" />
		<!-- 默认false，把JDBC decimal 和 numeric 类型解析为 Integer true，把JDBC decimal 和 
			numeric 类型解析为java.math.BigDecimal -->
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>
		<!-- targetPackage：生成实体类的位置， targetProject:自动生成代码的位置 -->
		<javaModelGenerator targetPackage="com.blog.model"
			targetProject="src/main/java">
			<!-- enableSubPackages:是否让schema作为包的后缀 -->
			<property name="enableSubPackages" value="true" />
			<!-- 从数据库返回的值被清理前后的空格 -->
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		<!-- targetPackage:放置生成的SQL映射文件 -->
		<sqlMapGenerator targetPackage="sql.blog"
			targetProject="src/main/java">
			<property name="enableSubPackages" value="false" /> <!-- enableSubPackages:是否让schema作为包的后缀 -->
		</sqlMapGenerator>
		<!-- type： targetPackage:接口的位置 -->
		<javaClientGenerator targetPackage="com.blog.dao"
			targetProject="src/main/java" type="XMLMAPPER">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>
        <table schema="" tableName="BIT_USER" domainObjectName="BitUser"
               enableCountByExample="false" enableUpdateByExample="false"
               enableDeleteByExample="false" enableSelectByExample="false"
               selectByExampleQueryId="false">
			<!-- 指定列的java数据类型 -->
			<columnOverride column="CREATETIME" jdbcType="TIMESTAMP" />
		</table>

	</context>
</generatorConfiguration>